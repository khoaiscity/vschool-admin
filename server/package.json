{
  "name": "@loopback/example-express-composition",
  "version": "1.5.4",
  "description": "LoopBack 4 REST API on Express",
  "keywords": [
    "loopback-application",
    "loopback",
    "example",
    "tutorial",
    "express"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-express-composition*.tgz dist tsconfig.build.tsbuildinfo package",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "pm2 start ecosystem.config.js --env production",
    "start:local": "npm run prestart && node .",
    "stop": "pm2 stop ecosystem.config.js --env production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "examples/express-composition"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/boot": "^1.5.3",
    "@loopback/context": "^1.21.4",
    "@loopback/core": "^1.9.3",
    "@loopback/openapi-v3": "^1.9.4",
    "@loopback/repository": "^1.12.0",
    "@loopback/rest": "^1.17.0",
    "@loopback/rest-explorer": "^1.3.4",
    "@loopback/service-proxy": "^1.3.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "p-event": "^4.1.0",
    "pm2": "^3.5.0"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.8",
    "@loopback/eslint-config": "^4.0.1",
    "@loopback/testlab": "^1.7.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/node": "^10.14.15",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^6.0.0",
    "http-proxy-middleware": "^0.19.1",
    "typescript": "^3.5.3"
  },
  "gitHead": "e17358d04cf9986fc692fdea37b582111932551d",
  "_resolved": "",
  "_integrity": "",
  "_from": "@loopback/example-express-composition@latest"
}
